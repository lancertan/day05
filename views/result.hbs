{{#each rets}}
<div class="row">
    <h4>{{ this.title }}</h4>
</div>
<div class="row">
    {{#if urlToImage}}
        <img src="{{ this.urlToImage }}" alt="" width="400">
    {{else}}
        <img src="placeholder.jpg" alt="" width="400">
    {{/if}}
</div>
<div class="row">
    <h5>{{ this.description }}</h5>
</div>
<div class="row">
    <div class="col">
        <h6>{{ this.publishedAt }}</h6>
    </div>
    <div class="col">
        <h5><a href="{{ this.url }}">Read</a></h5>
    </div>
</div>
{{/each}}